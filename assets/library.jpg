<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="#f0f4f8"/>
  
  <!-- Library interior walls -->
  <rect x="0" y="400" width="800" height="200" fill="#d1d5db"/>
  <rect x="0" y="0" width="800" height="400" fill="#e5e7eb"/>
  
  <!-- Bookshelves -->
  <rect x="50" y="100" width="150" height="300" fill="#8B4513"/>
  <line x1="50" y1="150" x2="200" y2="150" stroke="#654321" stroke-width="3"/>
  <line x1="50" y1="200" x2="200" y2="200" stroke="#654321" stroke-width="3"/>
  <line x1="50" y1="250" x2="200" y2="250" stroke="#654321" stroke-width="3"/>
  <line x1="50" y1="300" x2="200" y2="300" stroke="#654321" stroke-width="3"/>
  <line x1="50" y1="350" x2="200" y2="350" stroke="#654321" stroke-width="3"/>
  
  <!-- Books on shelves -->
  <rect x="60" y="110" width="20" height="35" fill="#1a5490"/>
  <rect x="85" y="110" width="15" height="35" fill="#d97706"/>
  <rect x="105" y="110" width="25" height="35" fill="#047857"/>
  <rect x="135" y="110" width="18" height="35" fill="#dc2626"/>
  <rect x="158" y="110" width="22" height="35" fill="#2563eb"/>
  
  <!-- Adjustable-height desk -->
  <rect x="300" y="350" width="200" height="10" fill="#8B4513"/>
  <rect x="320" y="360" width="15" height="90" fill="#696969"/>
  <rect x="465" y="360" width="15" height="90" fill="#696969"/>
  
  <!-- Up/down arrows on desk (indicating adjustable) -->
  <polygon points="400,370 395,380 405,380" fill="#1a5490"/>
  <polygon points="400,390 395,380 405,380" fill="#1a5490"/>
  
  <!-- Computer with screen reader -->
  <rect x="350" y="320" width="100" height="70" fill="#2c3e50"/>
  <rect x="360" y="330" width="80" height="50" fill="#87CEEB"/>
  <text x="400" y="360" font-family="Arial" font-size="12" fill="#000000" text-anchor="middle">SCREEN</text>
  <text x="400" y="375" font-family="Arial" font-size="12" fill="#000000" text-anchor="middle">READER</text>
  
  <!-- Wheelchair accessible space indicator -->
  <rect x="250" y="400" width="120" height="150" fill="none" stroke="#1a5490" stroke-width="4" stroke-dasharray="10,5"/>
  
  <!-- Wheelchair symbol -->
  <g transform="translate(310, 470)">
    <circle cx="0" cy="-20" r="12" fill="#1a5490"/>
    <path d="M -10 -5 Q -10 5, 0 10 T 10 -5" fill="#1a5490"/>
    <line x1="-15" y1="2" x2="-25" y2="15" stroke="#1a5490" stroke-width="4"/>
    <circle cx="-8" cy="20" r="18" fill="none" stroke="#1a5490" stroke-width="4"/>
    <circle cx="8" cy="20" r="18" fill="none" stroke="#1a5490" stroke-width="4"/>
  </g>
  
  <!-- Large text signage -->
  <rect x="550" y="150" width="200" height="100" fill="#ffffff" stroke="#1a5490" stroke-width="3"/>
  <text x="650" y="185" font-family="Arial" font-size="24" font-weight="bold" fill="#1a5490" text-anchor="middle">QUIET</text>
  <text x="650" y="215" font-family="Arial" font-size="24" font-weight="bold" fill="#1a5490" text-anchor="middle">STUDY</text>
  <text x="650" y="240" font-family="Arial" font-size="16" fill="#1a5490" text-anchor="middle">AREA</text>
  
  <!-- Braille dots (simplified representation) -->
  <circle cx="560" cy="170" r="3" fill="#1a5490"/>
  <circle cx="570" cy="170" r="3" fill="#1a5490"/>
  <circle cx="560" cy="180" r="3" fill="#1a5490"/>
  
  <!-- Window with natural light -->
  <rect x="600" y="50" width="150" height="200" fill="#87CEEB" opacity="0.6"/>
  <line x1="675" y1="50" x2="675" y2="250" stroke="#ffffff" stroke-width="4"/>
  <line x1="600" y1="150" x2="750" y2="150" stroke="#ffffff" stroke-width="4"/>
  
  <!-- Assistive technology station sign -->
  <rect x="50" y="50" width="180" height="40" fill="#1a5490"/>
  <text x="140" y="77" font-family="Arial" font-size="16" font-weight="bold" fill="#ffffff" text-anchor="middle">Assistive Tech Station</text>
  
  <!-- Floor pattern (tactile) -->
  <g opacity="0.3">
    <circle cx="100" cy="550" r="5" fill="#1a5490"/>
    <circle cx="120" cy="550" r="5" fill="#1a5490"/>
    <circle cx="140" cy="550" r="5" fill="#1a5490"/>
    <circle cx="160" cy="550" r="5" fill="#1a5490"/>
    <circle cx="180" cy="550" r="5" fill="#1a5490"/>
    <circle cx="200" cy="550" r="5" fill="#1a5490"/>
  </g>
</svg>
